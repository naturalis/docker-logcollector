{
"description": "Pipeline for parsing pfsense syslog messages.",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": [
          "%{GREEDYDATA:syslog_message}"
        ],
        "ignore_missing": true
      }
    },
    {
      "remove": {
        "field": "message"
      }
    },
    {
      "date": {
        "field": "syslog_timestamp",
        "target_field": "@timestamp",
        "formats": [
          "MMM  d HH:mm:ss",
          "MMM dd HH:mm:ss"
        ],
        "ignore_failure": true
      }
    },
    {
      "json": {
        "field" : "syslog_message",
        "target_field" : "syslog_message_aap",
        "ignore_failure" : true
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
